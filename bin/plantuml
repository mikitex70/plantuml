#!/bin/sh
# if first argument is "-resources" then is expected the resources path for plantuml include directives
#
resources=""
firstArg=$1

if [ "${firstArg}" = "-resources" ]; then
    resources="-Dplantuml.include.path=$2"
    shift 2
fi

java $resources -jar "$(dirname $0)/plantuml.8034.jar" ${@}
